                <p1:Density>0</p1:Density>
                <p1:OldDensity>0</p1:OldDensity>
                <p1:Drift>7.5</p1:Drift>
                <p1:OldDrift>7.5</p1:OldDrift>
                <p1:logLowFreq>8</p1:logLowFreq>
                <p1:logHighFreq>12</p1:logHighFreq>
                <p1:SinWaves>100</p1:SinWaves>
                <p1:SampleRate>44100</p1:SampleRate>
                <p1:Amplitude>0</p1:Amplitude>
                <p1:Offset>0</p1:Offset>
                <p1:Phase>0</p1:Phase>
              </Combinator>
            </Expression>
            <Expression xsi:type="Combinator">
              <Combinator xsi:type="rx:Take">
