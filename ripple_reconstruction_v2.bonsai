              <Combinator xsi:type="rx:Zip" />
            </Expression>
            <Expression xsi:type="scr:ExpressionTransform">
              <scr:Expression>new(
Item1.Item1 as Drift,
Item1.Item2 as Density,
Item1.Item3 as OldDrift,
Item1.Item4 as OldDensity,
Item1.Item5 as TimeOffset,
Item2 as CommonTime,
